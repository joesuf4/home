Host *
  KexAlgorithms ecdh-sha2-nistp521
  Ciphers aes256-ctr
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h:%p
  ControlPersist 9000
  ServerAliveInterval 60
  HashKnownHosts no
  User joe

Host 127.0.0.1
  LocalForward 127.0.0.1:4433 127.0.0.1:443

Host oci-*
  StrictHostKeyChecking no
  RemoteForward 127.0.0.1:2069 127.0.0.1:2069
  RemoteForward 127.0.0.1:4433 127.0.0.1:443
  RemoteForward 127.0.0.1:9999 127.0.0.1:9999
  RemoteForward 192.168.254.1:587 192.168.254.1:587
  RequestTTY yes

Host *.sunstarsys.com
  ForwardAgent yes
  RequestTTY yes

Host github.com
  StrictHostKeyChecking no
