Host *
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h:%p
  ControlPersist 900
  ServerAliveInterval 60

Host *-*
  ProxyCommand $SHELL -c ". ~/.awsrc; aws ssm start-session --profile \$AWS_PROFILE --target \$AWS_ID[%h] --document-name AWS-StartSSHSession --parameters 'portNumber=%p'"
  ForwardAgent yes
  StrictHostKeyChecking no
  User ssm-user
