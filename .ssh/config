Host *
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h:%p
  ControlPersist 900
  ServerAliveInterval 60
  HashKnownHosts no

Host *-*
  ProxyCommand $SHELL -c '. ~/.awsrc && aws ssm start-session --target $AWS_ID[%h] --document-name AWS-StartSSHSession --parameters portNumber=%p'
  ForwardAgent yes
  StrictHostKeyChecking no
  User ssm-user
